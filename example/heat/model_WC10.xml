
<?xml version="1.0"?>
<CLBConfig version="2.0" output="output/WC10_5/">

	<Units>
		<Params Size="0.04m"        gauge="160"/>
		<Params Nu="1e-5m2/s"       gauge="0.166666666"/>
		<Params Density="1e3kg/m3"  gauge="1"/>
	</Units>

	<Geometry nx="0.04m" ny="0.02m">
		<BGK>
			<Box/>
		</BGK>

		<DesignSpace>
			<Box/>
		</DesignSpace>

		<PseudoWall>
			<Box dy="0" ny="0.001m"/>
			<Box dy="-0.001m" ny="0.001m"/>
		</PseudoWall>

		<Wall mask="ALL">
			<Box dy="0" ny="1"/>
			<Box dy="-1" ny="1"/>
		</Wall>

		<WallSouth>
			<Box dy="0" ny="1"/>
		</WallSouth>

		<WallNorth>
			<Box dy="-1" ny="1"/>
		</WallNorth>

		<InletGauge>
			<Box dx="0" nx="1"/>
		</InletGauge>

		<OutletGauge>
			<Box dx="-1" nx="1"/>
		</OutletGauge>

		<InitHeater>
			<Box dy="0.015m" ny="0.001m" dx="0.0195m" nx="0.001m"/>
		</InitHeater>				
	</Geometry>

	<Model>
		<Params AlfaFluid="0.1666"/>
		<Params AlfaSolid="1.6666"/>
		<Params Q="1.39e-1"/>
		<Params Tref="300K"/>
		<Params W_init="0.0"/>
		<Params G_Boussinesq_Y="-9.81m/s2"/>
		<Params Ldim="0.02m"/>
		<Params Adiffusity="0.143mm2/s"/>
		<Params Nu_dup="1e-5m2/s"/>
		<Params FluidVolumeInObj="-1e-5"/>
		<Params MassFlowOutInObj="10.0"/>
		<Params MassFlowGlobalInObj="5.0"/>
		<!--Params PenaltyPorosityInObj="1e-3"/-->
		<Params InitSourceTemperature="350K"/>
	</Model>

	<VTK/>
	<Log Iterations="1000"/>
	<!--Stop Iterations="1000" MassFlowOutChange="1e-5" times="10"/-->
	<Solve Iterations="50000">
		<VTK Iterations="5000"/>
	</Solve>

	<VTK/>
	<FieldParameter field="w" where="DesignSpace"/>
	<Optimize method="MMA" MaxEvaluations="50">
        	<OptSolve Iterations="40000"/>
           	<VTK/>
   	</Optimize>

	<Params PenaltyPorosityInObj="5e-3"/>
	<FieldParameter field="w" where="DesignSpace"/>
	<Optimize method="MMA" MaxEvaluations="50">
		<OptSolve Iterations="4000"/>
		<VTK/>
	</Optimize>

	<Params PenaltyPorosityInObj="5e-2"/>
        <FieldParameter field="w" where="DesignSpace"/>
        <Optimize method="MMA" MaxEvaluations="20">
                <OptSolve Iterations="4000"/>
                <VTK/>
        </Optimize>

	<Solve Iterations="50000"/>
	<VTK/>

</CLBConfig>
