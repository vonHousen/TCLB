
<?xml version="1.0"?>
<CLBConfig version="2.0" output="output/WC8_6/">

	<Units>
		<Params Size="1e-4m"        gauge="1"/>
		<Params Nu="1e-5m2/s"       gauge="0.166666666"/>
		<Params Density="1e3kg/m3"  gauge="1"/>
	</Units>

	<Geometry nx="0.04m" ny="0.02m">
		<BGK>
			<Box/>
		</BGK>

		<Wall mask="ALL">
			<Box dy="0" ny="1"/>
			<Box dy="-1" ny="1"/>
		</Wall>

		<PseudoWall>
			<Box dx="0.015m" nx="0.01m" dy="0"       ny="0.003m"/>
		</PseudoWall>

		<InletGauge>
			<Box dx="0" nx="1"/>
		</InletGauge>

		<OutletGauge>
			<Box dx="-1" nx="1"/>
		</OutletGauge>

	</Geometry>

	<Model>
		<Params InitTemperature="300K"/>
		<Params InitVelocityX="0.08m/s"/>
		<Params AlfaFluid="0.1"/>
		<Params AlfaSolid="0.05"/>
		<Params Omega="1.0"/>
		<Params Tref="300K"/>
		<Params Nu_dup="1e-5m2/s"/>
		<Params W_init="0.5"/>
		<Params G_X="0.003m/s2"/>
	</Model>

	<VTK/>
	<Solve Iterations="3000"/>
	<VTK/>
	<!--Solve Iterations="3000">
		<VTK Iterations="100"/>
		<Log Iterations="100"/>
	</Solve-->


</CLBConfig>
