
<?xml version="1.0"?>
<CLBConfig version="2.0" output="output/WC9/">

	<Units>
		<Params Size="1e-4m"        gauge="1"/>
		<Params Nu="1e-5m2/s"       gauge="0.166666666"/>
		<Params Density="1e3kg/m3"  gauge="1"/>
	</Units>

	<Geometry nx="0.04m" ny="0.02m">
		<BGK>
			<Box/>
		</BGK>

		<Wall mask="ALL">
			<Box dy="0" ny="1"/>
			<Box dy="-1" ny="1"/>
		</Wall>

		<PseudoWall>
			<Box dy="0.009m" ny="0.002m"/>
		</PseudoWall>

		<WallSouth>
			<Box dy="0" ny="1"/>
		</WallSouth>

		<WallNorth>
			<Box dy="-1" ny="1"/>
		</WallNorth>

	</Geometry>

	<Model>
		<Params InitTemperature="300K"/>
		<Params AlfaFluid="4"/>
		<Params AlfaSolid="0.5"/>
		<Params Omega="1.0"/>
		<Params Tref="300K"/>
		<Params Nu_dup="1e-5m2/s"/>
		<Params W_init="0.5"/>
		<Params Q="1.39"/>
	</Model>

	<VTK/>
	<Solve Iterations="2500">
		<VTK Iterations="100"/>
		<Log Iterations="100"/>
	</Solve>


</CLBConfig>
