		<VTK/>
	</Optimize>

	<Params PenaltyPorosityInObj="5e-1"/>
	<FieldParameter field="w" where="DesignSpace"/>
	<Optimize method="MMA" MaxEvaluations="20" Material="more">
		<OptSolve Iterations="100000"/>
		<VTK/>
	</Optimize>

	<Solve Iterations="100000"/>
